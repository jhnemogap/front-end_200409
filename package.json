{
  "name": "front-end_exercise_200409",
  "version": "1.0.0",
  "description": "Front-end exercise 2020/04/09 by PlatziMaster",
  "main": "index.html",
  "scripts": {
    "dev": "parcel serve index.html --no-source-maps --no-cache",
    "cypress:open": "npx cypress open --project ./testCypress",
    "cypress:run": "npx cypress run --project ./testCypress",
    "test:dev": "run-p --race dev cypress:open",
    "test:gh-pages": "run-s build:gh-pages && run-p --race serve:build cypress:run",
    "serve:build": "npx serve -l 1234 build/",
    "build": "rm -rf build/ && parcel build index.html --no-source-maps --out-dir build/",
    "build:gh-pages": "rm -rf build/ && parcel build index.html --no-source-maps --out-dir build/ --public-url ./",
    "deploy:gh-pages": "npx gh-pages -d build -b gh-pages -f",
    "gh-pages:build:deploy": "run-s build:gh-pages deploy:gh-pages",
    "gh-pages:build:test:deploy": "run-s test:gh-pages deploy:gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhnemogap/front-end_exercise_200409.git"
  },
  "keywords": [
    "js",
    "vanilla",
    "html",
    "css"
  ],
  "author": "@jhnemogap",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhnemogap/front-end_exercise_200409/issues"
  },
  "homepage": "https://github.com/jhnemogap/front-end_exercise_200409#readme",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "cypress": "4.4.1",
    "gh-pages": "2.2.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "serve": "11.3.0"
  },
  "targets": "> 0.25%, not dead"
}
